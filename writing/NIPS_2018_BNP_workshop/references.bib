% Comparing clusters

@article{fowlkes:1983:method,
 ISSN = {01621459},
 URL = {http://www.jstor.org/stable/2288117},
 abstract = {This article concerns the derivation and use of a measure of
similarity between two hierarchical clusterings. The measure, B<sub>k</sub>,
is derived from the matching matrix, [ m<sub>ij</sub>], formed by cutting the
two hierarchical trees and counting the number of matching entries in the k
clusters in each tree. The mean and variance of B<sub>k</sub> are determined
under the assumption that the margins of [ m<sub>ij</sub>] are fixed. Thus,
B<sub>k</sub> represents a collection of measures for k = 2,..., n - 1. (k,
B<sub>k</sub>) plots are found to be useful in portraying the similarity of
two clusterings. B<sub>k</sub> is compared to other measures of similarity
proposed respectively by Baker (1974) and Rand (1971). The use of (k,
B<sub>k</sub>) plots for studying clustering methods is explored by a series
of Monte Carlo sampling experiments. An example of the use of (k,
B<sub>k</sub>) on real data is given.},
 author = {E. B. Fowlkes and C. L. Mallows},
 journal = {Journal of the American Statistical Association},
 number = {383},
 pages = {553-569},
 publisher = {[American Statistical Association, Taylor & Francis, Ltd.]},
 title = {A Method for Comparing Two Hierarchical Clusterings},
 volume = {78},
 year = {1983}
}
/

